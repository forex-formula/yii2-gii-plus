{
  "name": "ivan-chkv/yii2-gii-plus",
  "description": "Yii 2 Gii Plus Extension",
  "keywords": ["yii2", "gii", "code generator"],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Chistyakov",
      "email": "ivan.chkv@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": ">=2.0.4",
    "yiisoft/yii2-gii": "2.0.4",
    "ivan-chkv/yii2-jui-autocomplete": "*"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "codeception/codeception": "*"
  },
  "autoload": {
    "psr-4": {
      "yii\\gii\\plus\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
